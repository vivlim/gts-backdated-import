        {
            "request": "launch",
            "name": "Debug tests",
            "type": "node",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "test",
                "--inspect-wait"
