    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Gather from archive",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable-kv",
                "--inspect-wait",
                "--allow-read",
                "--allow-env",
                "--allow-net"
            ],
            "args": [
                "--exportPath",
                "data/archive-extracted",
                "--targetAcct",
                "test_a@electron.phosphor.buzz",
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Publish from DB",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable-kv",
                "--inspect-wait",
                "--allow-read",
                "--allow-env",
                "--allow-net"
            ],
            "args": [
                "--publish",
                "--targetAcct",
                "test_a@electron.phosphor.buzz",
            ],
            "attachSimplePort": 9229
        },
        {
            "request": "launch",
            "name": "Build backdating sql",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "deno",
            "runtimeArgs": [
                "run",
                "--unstable-kv",
                "--inspect-wait",
                "--allow-read",
                "--allow-write",
                "--allow-env",
                "--allow-net"
            ],
            "args": [
                "--backdating-query",
                "--targetAcct",
                "test_a@electron.phosphor.buzz",
